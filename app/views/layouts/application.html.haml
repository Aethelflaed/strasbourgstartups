!!!
%html
	%head
		%title Strasbourgstartups
		= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true
		= stylesheet_link_tag "http://fonts.googleapis.com/css?family=Oswald:300.400.700"
		= javascript_include_tag "application", "data-turbolinks-track" => true
		- api_key = "AIzaSyAw8mWBLhSsE2dI2t0dLhsZt12I9kSEKK0"
		= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{api_key}&sensor=false"
		= csrf_meta_tags
%body
	%nav
		Strasbourg Startups
		= link_to new_startup_path, class: "add", target: "_blank", title: "Ajouter une startup" do
			%span.icon-plus
		= link_to headlines_path, class: "headlines", target: "_blank", title: "News" do
			%span.icon-rss
	= yield
